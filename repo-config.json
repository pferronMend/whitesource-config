{
  "overrideConfigAllowList": [],
  "scanSettings": {
    "configMode": "AUTO",
    "configExternalURL": "",
    "baseBranches": [],
    "enableLicenseViolations": true,
    "enableReachability": true
  },
  "checkRunSettings": {
    "vulnerableCheckRunConclusionLevel": "failure",
    "strictMode": "failure",
    "displayMode": "diff",
    "useMendCheckNames": true
  },
  "issueSettings": {
    "minSeverityLevel": "LOW",
    "displayLicenseViolations": true,
    "issueType": "DEPENDENCY",
    "customLabels": ["prevent-stale"]
  },
  "leastVulnerablePackageSettings": {                
   "enabled": true
  },
  "remediateSettings": {
    "workflowRules": {
      "enabled": true,
      "minVulnerabilitySeverity": "LOW"
    },
    "extends": [
      "github>whitesource/merge-confidence:beta"
    ]
  }
}
